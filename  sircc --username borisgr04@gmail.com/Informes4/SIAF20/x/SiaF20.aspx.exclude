                $("#jqxgridF20_1a").jqxGrid('exportdata', 'xls', 'fmF20_1a');
            });
            $("#excelExport_b").click(function () {
                $("#jqxgridF20_1b").jqxGrid('exportdata', 'xls', 'fmF20_1b');
            });
            $("#excelExport_c").click(function () {
                $("#jqxgridF20_1c").jqxGrid('exportdata', 'xls', 'fmF20_1c');
