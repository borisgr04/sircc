            var theme = getDemoTheme();

            function bloquear() {
                $.blockUI();
            }
            function desbloquear() {
                $.unblockUI();
            }

            $('#btnAceptar').click(function () {
                F20_1a();
                F20_1b();
                F20_1c();
            });

